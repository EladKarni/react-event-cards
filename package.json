{
  "name": "react-event-cards",
  "version": "0.1.6",
  "description": "A simple component that let's you create cards of upcoming events from a public Google calendar.",
  "main": "lib/index.js",
  "scripts": {
    "dev": "concurrently \"npm run lib:watch\" \"npm run docs\"",
    "lib": "babel src/lib -d lib --copy-files",
    "lib:watch": "babel src/lib -w -d lib --copy-files",
    "docs": "webpack-dev-server --mode development",
    "docs:prod": "webpack --mode production"
  },
  "keywords": [],
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.3.0 || ^16.2.0",
    "react-dom": "^15.3.0 || ^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "concurrently": "^3.6.1",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.2.1"
  },
  "author": "Elad Karni <contact@eladkarni.com>",
  "homepage": "https://github.com/eladkarni/react-event-cards",
  "repository": {
    "type": "git",
    "url": "git@github.com/eladkarni/react-event-cards.git"
  },
